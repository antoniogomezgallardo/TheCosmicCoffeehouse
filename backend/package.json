{
  "name": "cosmic-coffeehouse-backend",
  "version": "1.1.0",
  "description": "Backend API for The Cosmic Coffeehouse - Superpower Coffee E-commerce",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "seed": "ts-node src/scripts/seed.ts",
    "clean": "rm -rf dist",
    "docs": "start http://localhost:3001/api/docs",
    "docs:json": "start http://localhost:3001/api/docs.json",
    "api-info": "start http://localhost:3001/api"
  },
  "keywords": [
    "ecommerce",
    "api",
    "typescript",
    "mongodb",
    "express",
    "testing"
  ],
  "author": "Antonio Gomez Gallardo",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.1",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.1.3",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
